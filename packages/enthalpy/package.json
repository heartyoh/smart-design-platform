{
  "name": "@smart-design-platform/enthalpy",
  "version": "0.0.15",
  "main": "dist-server/index.js",
  "browser": "dist-client/index.js",
  "things-factory": true,
  "license": "MIT",
  "author": "",
  "description": "Energy calculation logic based on enthalpy flow.",
  "publishConfig": {
    "access": "public",
    "@smart-design-platform:registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hatiolab/smart-design-platform.git",
    "directory": "packages/enthalpy"
  },
  "scripts": {
    "build": "npm run build:server && npm run build:client",
    "build:client": "tsc --p tsconfig.client.json",
    "build:server": "tsc --p tsconfig.server.json",
    "clean:client": "rm -rf dist-client",
    "clean:server": "rm -rf dist-server",
    "clean": "npm run clean:server && npm run clean:client",
    "migration:create": "node ../../node_modules/typeorm/cli.js migration:create -d ./server/migrations",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "@material/mwc-list": "^0.25.3",
    "@material/mwc-select": "^0.25.3",
    "@operato/graphql": "^1.0.0-beta.7",
    "@operato/i18n": "^1.0.0-beta.7",
    "@operato/layout": "^1.0.0-beta.7",
    "@operato/popup": "^1.0.0-beta.7",
    "@operato/shell": "^1.0.0-beta.7",
    "@operato/utils": "^1.0.0-beta.7",
    "@things-factory/env": "5.0.0-alpha.40",
    "@things-factory/shell": "5.0.0-alpha.40",
    "read-excel-file": "^5.2.26"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "regenerator-runtime": "^0.13.9",
    "ts-jest": "^27.1.3"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./tests/setup.js"
    ]
  }
}
