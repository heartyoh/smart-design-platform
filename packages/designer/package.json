{
  "name": "@smart-design-platform/designer",
  "version": "0.0.14",
  "main": "dist-server/index.js",
  "browser": "client/index.js",
  "things-factory": true,
  "license": "MIT",
  "author": "heartyoh",
  "description": "process designer application",
  "publishConfig": {
    "access": "public",
    "@smart-design-platform:registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heartyoh/smart-design-platform.git",
    "directory": "packages/designer"
  },
  "scripts": {
    "serve": "things-factory sdp",
    "serve:dev": "npm run build:server && things-factory-dev sdp",
    "build": "tsc --p tsconfig.json",
    "build:client": "npm run clean:client && webpack --config ../../node_modules/@things-factory/builder/webpack.config.js",
    "build:server": "npm run clean:server && tsc",
    "clean:server": "rm -rf dist-server",
    "clean:client": "rm -rf dist-client",
    "clean": "npm run clean:server && npm run clean:client",
    "migration": "npm run build:server && things-factory-migration",
    "migration:privileges": "npm run build:server && things-factory-migration-privileges",
    "migration:create": "node ../../node_modules/typeorm/cli.js migration:create -d ./server/migrations",
    "migration:generate": "node ../../node_modules/typeorm/cli.js migration:generate",
    "migration:run": "node ../../node_modules/typeorm/cli.js migration:run",
    "migration:revert": "node ../../node_modules/typeorm/cli.js migration:revert",
    "stop:dev": "kill $(lsof -t -i:3000,3001)",
    "stop": "things-factory-stop sdp",
    "docker": "things-factory-dockerize hatiolab/sdp:latest hatiolab/sdp:$npm_package_version",
    "docker:push": "docker image push hatiolab/sdp:latest && docker image push hatiolab/sdp:$npm_package_version",
    "docker:run": "docker run -it -p 4000:3000 hatiolab/sdp:latest"
  },
  "dependencies": {
    "@operato/data-grist": "^1.0.0-beta.5",
    "@operato/graphql": "^1.0.0-beta.5",
    "@operato/i18n": "^1.0.0-beta.5",
    "@operato/input": "^1.0.0-beta.5",
    "@operato/popup": "^1.0.0-beta.5",
    "@operato/shell": "^1.0.0-beta.5",
    "@operato/utils": "^1.0.0-beta.5",
    "@smart-design-platform/enthalpy": "^0.0.14",
    "@smart-design-platform/equipment": "^0.0.14",
    "@smart-design-platform/evaluator": "^0.0.14",
    "@smart-design-platform/process-modeller": "^0.0.14",
    "@things-factory/api": "5.0.0-alpha.38",
    "@things-factory/apptool-ui": "5.0.0-alpha.38",
    "@things-factory/attachment-base": "5.0.0-alpha.38",
    "@things-factory/auth-ui": "5.0.0-alpha.38",
    "@things-factory/board-service": "5.0.0-alpha.38",
    "@things-factory/context-ui": "5.0.0-alpha.38",
    "@things-factory/env": "5.0.0-alpha.38",
    "@things-factory/export-ui": "5.0.0-alpha.38",
    "@things-factory/export-ui-excel": "5.0.0-alpha.38",
    "@things-factory/fav-base": "5.0.0-alpha.38",
    "@things-factory/help": "5.0.0-alpha.38",
    "@things-factory/integration-ui": "5.0.0-alpha.38",
    "@things-factory/lite-menu": "5.0.0-alpha.38",
    "@things-factory/more-ui": "5.0.0-alpha.38",
    "@things-factory/notification": "5.0.0-alpha.38",
    "@things-factory/oauth2-client": "5.0.0-alpha.38",
    "@things-factory/print-ui": "5.0.0-alpha.38",
    "@things-factory/resource-ui": "5.0.0-alpha.38",
    "@things-factory/setting-base": "5.0.0-alpha.38",
    "@things-factory/setting-ui": "5.0.0-alpha.38",
    "@things-factory/shell": "5.0.0-alpha.38",
    "@things-factory/system-ui": "5.0.0-alpha.38"
  },
  "devDependencies": {
    "@things-factory/builder": "5.0.0-alpha.38",
    "@types/codemirror": "^5.60.5"
  }
}