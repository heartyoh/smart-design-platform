{
  "name": "@smart-design-platform/designer",
  "version": "0.0.7",
  "main": "dist-server/index.js",
  "browser": "client/index.js",
  "things-factory": true,
  "license": "MIT",
  "author": "heartyoh",
  "description": "process designer application",
  "publishConfig": {
    "access": "public",
    "@smart-design-platform:registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/heartyoh/smart-design-platform.git",
    "directory": "packages/designer"
  },
  "scripts": {
    "serve": "things-factory designer",
    "serve:dev": "npm run build:server && things-factory-dev designer",
    "build": "tsc --p tsconfig.json",
    "build:client": "npm run clean:client && webpack --config ../../node_modules/things-factory/builder/webpack.config.js",
    "build:server": "npm run clean:server && tsc",
    "clean:server": "rm -rf dist-server",
    "clean:client": "rm -rf dist-client",
    "clean": "npm run clean:server && npm run clean:client",
    "migration": "npm run build:server && things-factory-migration",
    "migration:privileges": "npm run build:server && things-factory-migration-privileges",
    "migration:create": "node ../../node_modules/typeorm/cli.js migration:create -d ./server/migrations",
    "migration:generate": "node ../../node_modules/typeorm/cli.js migration:generate",
    "migration:run": "node ../../node_modules/typeorm/cli.js migration:run",
    "migration:revert": "node ../../node_modules/typeorm/cli.js migration:revert",
    "stop:dev": "kill $(lsof -t -i:3000,3001)",
    "stop": "things-factory-stop designer",
    "docker": "things-factory-dockerize hatiolab/designer:latest",
    "docker:run": "docker run -it -p 4000:3000 hatiolab/designer:latest"
  },
  "dependencies": {
    "@things-factory/api": "4.0.0-alpha.0",
    "@things-factory/apptool-ui": "4.0.0-alpha.0",
    "@things-factory/auth-ui": "4.0.0-alpha.0",
    "@things-factory/board-service": "4.0.0-alpha.0",
    "@things-factory/board-ui": "4.0.0-alpha.0",
    "@things-factory/context-ui": "4.0.0-alpha.0",
    "@things-factory/dashboard": "4.0.0-alpha.0",
    "@things-factory/export-ui": "4.0.0-alpha.0",
    "@things-factory/export-ui-excel": "4.0.0-alpha.0",
    "@things-factory/grist-ui": "4.0.0-alpha.0",
    "@things-factory/help": "4.0.0-alpha.0",
    "@things-factory/i18n-ui": "4.0.0-alpha.0",
    "@things-factory/integration-ui": "4.0.0-alpha.0",
    "@things-factory/lite-menu": "4.0.0-alpha.0",
    "@things-factory/more-ui": "4.0.0-alpha.0",
    "@things-factory/notification": "4.0.0-alpha.0",
    "@things-factory/oauth2-client": "4.0.0-alpha.0",
    "@things-factory/print-ui": "4.0.0-alpha.0",
    "@things-factory/resource-ui": "4.0.0-alpha.0",
    "@things-factory/scene-chartjs": "4.0.0-alpha.0",
    "@things-factory/scene-clock": "4.0.0-alpha.0",
    "@things-factory/scene-clone": "4.0.0-alpha.0",
    "@things-factory/scene-compass": "4.0.0-alpha.0",
    "@things-factory/scene-data-transform": "4.0.0-alpha.0",
    "@things-factory/scene-form": "4.0.0-alpha.0",
    "@things-factory/scene-gauge": "4.0.0-alpha.0",
    "@things-factory/scene-graphql": "4.0.0-alpha.0",
    "@things-factory/scene-grist": "4.0.0-alpha.0",
    "@things-factory/scene-half-roundrect": "4.0.0-alpha.0",
    "@things-factory/scene-indoor-map": "4.0.0-alpha.0",
    "@things-factory/scene-integration": "4.0.0-alpha.0",
    "@things-factory/scene-label": "4.0.0-alpha.0",
    "@things-factory/scene-legend": "4.0.0-alpha.0",
    "@things-factory/scene-marker": "4.0.0-alpha.0",
    "@things-factory/scene-mpi": "4.0.0-alpha.0",
    "@things-factory/scene-mqtt": "4.0.0-alpha.0",
    "@things-factory/scene-news-ticker": "4.0.0-alpha.0",
    "@things-factory/scene-progressbar": "4.0.0-alpha.0",
    "@things-factory/scene-random": "4.0.0-alpha.0",
    "@things-factory/scene-restful": "4.0.0-alpha.0",
    "@things-factory/scene-switch": "4.0.0-alpha.0",
    "@things-factory/scene-tab": "4.0.0-alpha.0",
    "@things-factory/scene-table": "4.0.0-alpha.0",
    "@things-factory/scene-timer": "4.0.0-alpha.0",
    "@things-factory/setting-base": "4.0.0-alpha.0",
    "@things-factory/setting-ui": "4.0.0-alpha.0",
    "@things-factory/shell": "4.0.0-alpha.0",
    "@things-factory/system-ui": "4.0.0-alpha.0"
  },
  "devDependencies": {
    "@things-factory/builder": "^4.0.0-alpha.0"
  }
}
